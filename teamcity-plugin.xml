<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>teamcity-vsix-gallery</name>
    <display-name>teamcity-vsix-gallery</display-name>
    <version>@Version@</version>
    <description>A plugin for JetBrains TeamCity that turns it into a Visual Studio Extensions (vsix) Gallery</description>
    <download-url>https://github.com/hmemcpy/teamcity-vsix-gallery</download-url>
    <email>hmemcpy@gmail.com</email>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
      <logo>Plugin vendor logo URL</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
